{
    "name": "citizen-center",
    "version": "1.0.0",
    "private": true,
    "description": "Initial demo of what a Citizen Center could look like.",
    "author": "Ricky Lalwani",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt",
        "generate": "nuxt generate",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "start": "nuxt start",
        "test": "jest",
        "test:ci": "jest --coverage",
        "test:watch": "jest test --watch",
        "prettier": "prettier --write ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged, prettier"
        }
    },
    "lint-staged": {
        "*.{js,vue}": "yarn lint"
    },
    "dependencies": {
        "@azure/msal-browser": "^2.6.0",
        "@nuxtjs/axios": "^5.3.6",
        "@nuxtjs/dotenv": "^1.4.0",
        "@nuxtjs/pwa": "^3.0.2",
        "nuxt": "^2.14.7",
        "uswds": "^2.9.0"
    },
    "devDependencies": {
        "@hursey013/tailwindcss-uswds": "^1.2.0",
        "@nuxt/types": "~2.14.7",
        "@nuxtjs/eslint-config": "^4.0.0",
        "@nuxtjs/eslint-module": "^3.0.0",
        "@nuxtjs/tailwindcss": "^3.2.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.1",
        "eslint": "^7.12.1",
        "eslint-config-developit": "^1.2.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-nuxt": ">=1.0.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.2",
        "nuxt-purgecss": "^1.0.0",
        "prettier": "^2.1.2"
    }
}
